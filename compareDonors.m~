function problems = compareData(samples)
    [p,q] = meshgrid(1: size(samples,2), 1: size(samples,2));
    pairs = [p(:) q(:)];

    pairs = unique(sort(pairs,2),'rows');
    textCategories = size(samples{1}.textTitles,2);
        textCategories = size(samples{1}.textTitles,2);

    textProblems = cell(textCategories,1);
    
    problems = cell(7,1);
    for i =1:size(pairs,1);
        firstIndex = pairs(i,1);
        secondIndex = pairs(i,2);
        for j=1:size(textCategories)
            textProblems{j} = [textProblems{j}, find(~strcmp(samples{firstIndex}.textData(:,j), samples{secondIndex}.textData(:,j)))];
        end
        
        for j=1:size(textCategories)
            textProblems{j} = [textProblems{j}, find(~strcmp(samples{firstIndex}.textData(:,j), samples{secondIndex}.textData(:,j)))]
        end
        problems{1} = [problems{1},find(~strcmp(samples{firstIndex}.probe_ids, samples{secondIndex}.probe_ids))];
        problems{2} = [problems{2},find(~strcmp(samples{firstIndex}.probe_names, samples{secondIndex}.probe_names))];
        problems{3} = [problems{3},find(~strcmp(samples{firstIndex}.gene_ids, samples{secondIndex}.gene_ids))];
        problems{4} = [problems{4},find(~strcmp(samples{firstIndex}.gene_symbols, samples{secondIndex}.gene_symbols))];
        problems{5} = [problems{5},find(~strcmp(samples{firstIndex}.gene_names, samples{secondIndex}.gene_names))];
        problems{6} = [problems{6},find(~(samples{firstIndex}.entrez_id == samples{secondIndex}.entrez_id))];
        problems{7} = [problems{7},find(~(samples{firstIndex}.chromosome == samples{secondIndex}.chromosome))];
    end
end